
<!DOCTYPE html>
<html lang="en">

<%- include("../style.ejs") %>
<body>
  <div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
  
    <!-- partial -->

    <%- include("../head1.ejs") %>
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
     
      <!-- partial -->
     
      <%- include("../sidebar") %>
     
      <div class="main-panel">
        <div class="content-wrapper">


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Participants Details</h4>
        
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>
                  Name
                </th>
                <th>
                  Email
                </th>
                <th>
                  Contact
                </th>
                <th>
                  Status
                </th>
              
              </tr>
            </thead>
            <tbody>
                <% for(let i=0;i < detail.length; i++) { %>
                    <% detail[i].Sub_Events.participation.forEach(data1=>{ %>
                   
              <tr>
                <td class="py-1">
                  <%= data1.student_name %>
                </td>
             
                <td>
                  <%= data1.email %>
                </td>
                <td>
                  <%= data1.contact %>
                </td>
                <td>
                  <% if (data1.student_status==true || data1.student_status==on) { %>
                   BIT
                   <% } else {%>
                   NON-BIT 
                   <% } %>
                </td>
              </tr>
              <% })  }%>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <%- include("../footer") %>
  </div>
  </div>
</div>
</div>
</body>
</html>